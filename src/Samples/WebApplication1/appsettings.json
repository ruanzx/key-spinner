{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeySpinner": {
    "keys": [
      "key1",
      "key2",
      "key3"
    ],
    "rateLimitPerMinute": 5,
    "rateLimitPerHour": 50,
    "rateLimitPerDay": 200,
    "rateLimitPerMonth": 1000
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379,abortConnect=false,ssl=false"
  }
}
